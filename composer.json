{
    "name": "nggiahao/tessa",
    "description": "Quickly build an admin interfaces using Laravel 7, Livewire, AlpineJS and Tailwindcss.",
    "license": "",
    "authors": [
        {
            "name": "Nguyen Gia Hao",
            "email": "giahao9899@gmail.com"
        }
    ],
    "keywords": [
        "Laravel",
        "Tessa",
        "Tessa Laravel",
        "Tailwindcss",
        "Livewire",
        "Alpine.js"
    ],
    "require": {
        "php": "^7.3",
        "laravel/framework": "^7.0",
        "creativeorange/gravatar": "~1.0",
        "guzzlehttp/guzzle": "^6.3",
        "livewire/livewire": "^1.3",
        "laravel/ui": "^2.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5",
        "mockery/mockery": "^1.3.1",
        "orchestra/testbench": "~5",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Nggiahao\\Tessa\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Nggiahao\\Tessa\\Tests\\": "tests"
        },
        "files": ["src/helpers.php"]
    },
    "scripts": {
        "test": "vendor/bin/phpunit --testdox"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Nggiahao\\Tessa\\TessaServiceProvider"
            ]
        }
    }
}
